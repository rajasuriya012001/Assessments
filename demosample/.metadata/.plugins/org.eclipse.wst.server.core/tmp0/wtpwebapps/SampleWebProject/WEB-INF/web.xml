<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/java" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>SampleWebProject</display-name>
  <welcome-file-list>
    <welcome-file>sample.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>FirstServlet1</servlet-name>
  <servlet-class>com.training.Demos.FirstServlet1</servlet-class>
  <init-param>
  <param-name>sooon</param-name>
  <param-value>aakash</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>FirstServlet1</servlet-name>
  <url-pattern>/FirstServlet1</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>WelcomeServlet</servlet-name>
  <servlet-class>com.training.Demos.WelcomeServlet</servlet-class>
  <init-param>
  <param-name>sooon</param-name>
  <param-value>aakash</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>WelcomeServlet</servlet-name>
  <url-pattern>/Welcome</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>DemoSessionServlet1</servlet-name>
  <servlet-class>com.training.Demos1.DemoSessionServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>DemoSessionServlet1</servlet-name>
  <url-pattern>/DemoSession1</url-pattern>
  </servlet-mapping>
  
  <context-param>
  <param-name>greetings</param-name>
  <param-value> thank youuu</param-value>
  </context-param>
</web-app>