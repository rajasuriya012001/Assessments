package com.training.IOStream;

public enum Nume 
{
	public static final SUNDAY;
	public static final MONDAY;
	public static final TUESDAY;
	public static final WEDNESDAY;
	public static final THUYRSDAY;
	public static final FRIDAY;
	public static final SATURDAY ;

public static void main(String[]args)
{
	//enum Day{SUNDAY,MONDAY,TUEDAY,WEDNESDAY,THUYRSDAY,FRIDAY,SATURDAY};
	for(Day a:Day.values())
		
	{
		System.out.println(a);
	}
	System.out.println("value of sunday is "+ Day.valueOf("SUNDAY"));
	System.out.println("value of sunday is "+ Day.valueOf("SUNDAY"));
	
}
}