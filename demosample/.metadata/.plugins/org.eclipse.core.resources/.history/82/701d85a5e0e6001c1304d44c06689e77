import java.util.Scanner;

public class CircleMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double sum=0;
		int a[]=new int[100];
		for(int i=0;i<a.length;i++) {
			a[i]=(int)(Math.random()*10);
			Circle c= new Circle(a[i]);
			sum=sum+c.findArea();
		}


	}
	

}
