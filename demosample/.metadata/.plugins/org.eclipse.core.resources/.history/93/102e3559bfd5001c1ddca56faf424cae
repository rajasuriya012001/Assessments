package com.training.third;

public class Student1 extends Person
{
int roll,marks;
String course,stdadd;
public Student1()
{
	roll=123;
	marks=1234;
	course="java";
	stdadd="gokul nagar,sozhanganallur";
	
}
public Student1(int roll,int marks,String course,String stdadd,int phn,String name)
{
	super(phn,name);
	this.roll=roll;
	this.marks=marks;
	this.course=course;
	this.stdadd=stdadd;
}
public String display()
{
	System.out.println(super.display());
	return "ROLL NUMBER IS  :" + roll +"\n"+  "MARKS :"  + marks +"\n" +  "COURSE :"  + course+"\n" +  "STUDENT ADDRESS :" + stdadd;
}

}
