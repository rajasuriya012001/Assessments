package com.training.IOStream;
import java.util.*;

public class QuProgram {
	public static void main(String[]args)
	{
		PriorityQueue<Integer> qu = new PriorityQueue<Integer>();
		qu.add(3);
		qu.add(2);
		qu.add(1);
		qu.add(0);
		System.out.println("element in the head " +  qu.peek());
		qu.poll();
		System.out.println("element in the head " +  qu.peek());
		qu.remove();
		Iterator i = qu.iterator();
		while(i.hasNext())
		{
			System.out.println(i.next());
		}
		
		
	}

}
