package Assesment3;

public class CarMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car c=new Car(2345, 2022);
		c.setRegisterNumber(123);
		c.setModel(2010);
		System.out.println("regNo is "+c.getRegisterNumber());
		System.out.println("model is "+c.getModel());

		Car c1=new Car(7890, 9876);
		c1.setRegisterNumber(124);
		c1.setModel(2015);
		System.out.println("regNo is "+c1.getRegisterNumber());
		System.out.println("model is "+c1.getModel());

		Car c2=new Car(8973, 2021);
		c2.setRegisterNumber(125);
		c2.setModel(2016);
		System.out.println("regNo is "+c2.getRegisterNumber());
		System.out.println("model is "+c2.getModel());

		System.out.println("total count "+c.getCount());
	}

}
