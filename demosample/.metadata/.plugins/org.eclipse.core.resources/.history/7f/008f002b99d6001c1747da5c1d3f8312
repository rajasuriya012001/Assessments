			ArrayList z=new ArrayList();
			z.add(1234);
			z.add("hi");
			z.add("hello");
			z.add(3, "hi");
			Iterator i = z.iterator();
			while(i.hasNext())
			{
				System.out.println(i.next());
			}
			
			ArrayList<String> y=new ArrayList<String>();
			y.add("mic");
			y.add("testing");
			y.add("hello");
			Iterator i1 = y.iterator();
			while(i1.hasNext())
			{
				
				System.out.println(i1.next());
			}
			
