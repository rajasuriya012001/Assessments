package com.training.four;

public class Triangle extends Shape{
	int b,h;
	public void getdetails()
	{
		System.out.println("ENTER THE TRAINGLE VALUE :");
	}
	public void getdetails(int b,int h)
	{
		this.b=b;
		this.h=h;
	}
	public double calculateArea()
	{
		area=(b*h)/2;
		return area;
	}
	public String displayDetails()
	{
		return "Area of triangle " +area;
	}
	
	
	


}
