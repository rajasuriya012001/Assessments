package com.training;

import java.util.Scanner;

public class Accept {
	public static void main(String[]args) 
	{
		int sum=0;
	Scanner w = new Scanner(System.in);
	int arr1[]=new int [10];
	
	System.out.println("enter the value");
	for(int i=0;i<arr1.length;i++)
	{
		arr1[i]=w.nextInt();
		
	}	
	for(int j=0;j<arr1.length;j++)
	{
		
		sum=sum+arr1[j];
		
		
		
	}
	
	int avg=sum/arr1.length;
	
	
	//System.out.println("below of array value is "+arr1.length);
	System.out.println("sum of array value is "+sum);	
	System.out.println("average array  value is "+avg);	
	
	for(int j=0;j<avg-1;j++)
	{
		System.out.println("below of array value is "+arr1[j]);
	}
	for(int j=0;j<arr1.length;j++)
	{
		if(avg<arr1[j])
		{
			System.out.println("Above of array value is "+arr1[j]);
		}
		
	}
	for(int j=0;j<arr1.length;j++)
	{
		if(arr1[0]<arr1[j])
		{
			arr1[0]=arr1[j];
		
			System.out.println("minimum  of array value is "+arr1[0]);
		}
		
	
	
	
	
	
	
	

	


}
}
	


