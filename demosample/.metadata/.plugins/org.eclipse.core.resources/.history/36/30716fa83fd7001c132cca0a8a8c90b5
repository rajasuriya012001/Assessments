package com.training.IOStream;

import java.util.*;

import com.training.first.BankAccount;

public class NewArray {
	public static void main(String[]args)
	{
		ArrayList<BankAccount> al= new ArrayList<BankAccount>();
		al.add(new BankAccount());
		Iterator i = al.iterator();
		while(i.hasNext())
		{
			System.out.println(((BankAccount)i.next()).display());
		}
				
	}

}
